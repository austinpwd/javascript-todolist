{
  "name": "react-simpletodoapp",
  "version": "1.0.0",
  "description": "A simple To-Do List web application that is made using React and asynchronously parses from a JSON file.",
  "repository": {
    "type": "git",
    "url": "https://github.com/austinpwd/react-simpletodoapp.git"
  },
  "default": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run static && npx parcel src/index.html",
    "static": "mkdir -p dist && cp -R src/static/data dist/data",
    "dev": "",
    "build": "npm run static && npx parcel build src/index.html",
    "clean:dev": "rm -rf .parcel-cache dist",
    "clean:git": "rm -rf .parcel-cache dist  node_modules"
  },
  "license": "ISC",
  "keywords": [
    "to-do list",
    "austin doan",
    "assignment"
  ],
  "bugs": {
    "url": "https://github.com/austinpwd/react-simpletodoapp/issues"
  },
  "homepage": "https://github.com/austinpwd/react-simpletodoapp/#readme",
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0",
    "http-server": "^13.0.2",
    "parcel": "^2.0.0-rc.0",
    "sass": "^1.42.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^3.4.0"
  }
}