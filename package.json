{
  "name": "workflow",
  "version": "1.0.0",
  "description": "Fewder is a simple Front End Framework for developing Web App's using modules",
  "directories": {
    "doc": "docs"
  },
  "default": "src/index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx parcel src/index.html",
    "build": "npx parcel build src/index.html --dist-dir public",
    "clean:dev": "rm -rf .parcel-cache public",
    "clean:git": "rm -rf .parcel-cache public  node_modules"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-sass": "^2.0.0-rc.0",
    "http-server": "^13.0.2",
    "parcel": "^2.0.0-rc.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "sass": "^1.42.1"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "uuid": "^3.4.0"
  },
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "src/data/", 
        "staticOutDir": "data/" 
      }
    ]
  }
}
